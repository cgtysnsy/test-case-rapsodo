{
  "name": "rapsodo-fed-test-case",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "t": "cross-env NODE_OPTIONS=--experimental-vm-modules vue-cli-service test:unit --coverage --watchAll",
    "test:unit": "vitest --environment jsdom --root src"
  },
  "dependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.21.2",
    "@fortawesome/vue-fontawesome": "^3.0.3",
    "@mdi/font": "^7.1.96",
    "@rollup/pluginutils": "^5.0.2",
    "@types/jsdom": "^21.1.0",
    "core-js": "^3.8.3",
    "identity-obj-proxy": "^3.0.0",
    "jest-esm-transformer": "^1.0.0",
    "jest-transform-stub": "^2.0.0",
    "nanoid": "^4.0.1",
    "start-server-and-test": "^2.0.0",
    "ts-jest": "^27.1.5",
    "typescript": "^4.9.5",
    "vitest": "^0.29.2",
    "vue": "^3.2.13",
    "vue-router": "^4.0.13",
    "vue-template-compiler": "^2.7.14",
    "vuetify": "^3.1.4",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/eslint-parser": "^7.12.16",
    "@babel/preset-env": "^7.20.2",
    "@fortawesome/fontawesome-free": "^6.3.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-unit-jest": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/compiler-sfc": "^3.2.47",
    "@vue/test-utils": "^2.3.0",
    "@vue/vue3-jest": "^27.0.0",
    "babel-jest": "^27.5.1",
    "cross-env": "^7.0.3",
    "cypress": "^12.7.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "jest": "^27.5.1",
    "sass": "^1.58.3",
    "sass-loader": "^13.2.0",
    "vite-plugin-css-modules": "^0.0.1",
    "vite-plugin-vue-layouts": "^0.8.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "@vue/vue3-jest": {
    "transform": {
      "^.+\\.vue$": "vue-jest",
      "^.+\\.js$": "babel-jest"
    },
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.js",
      "<rootDir>/src/**/*.vue",
      "!<rootDir>/src/App.vue",
      "!<rootDir>/src/components/BasketPage.vue",
      "!<rootDir>/src/components/ProductPage.vue",
      "!<rootDir>/src/components/BasketIcon.vue",
      "!<rootDir>/src/plugins/vuetify.js",
      "!<rootDir>/src/store/index.js",
      "!<rootDir>/src/main.js",
      "!<rootDir>/src/router.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "coverageReporters": [
      "html",
      "text"
    ]
  }
}
